{
  "name": "escape-string-regexp-browser",
  "description": "Escape RegExp special characters",
  "author": "Max Degterev <max@nebenan.de>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "repository": "good-hood-gmbh/escape-string-regexp-browser",
  "bugs": "https://github.com/good-hood-gmbh/escape-string-regexp-browser/issues",
  "version": "1.0.0",
  "keywords": [
    "escape-string-regexp",
    "regexp",
    "browser"
  ],
  "main": "lib/index.js",
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "check": "npm run lint && echo \"\\033[0;32mReady to publish\\033[0m\"",
    "clean": "rm -rf ./lib/ || exit 1",
    "compile": "NODE_ENV=production babel src --out-dir lib --copy-files",
    "minify": "find ./lib -name '*.js' -exec terser {} -o {} --config-file terser.json \\;",
    "build": "npm run compile && npm run minify",
    "prepublishOnly": "npm run check && npm run clean && npm run build",
    "lint": "eslint ./",
    "lint:tofile": "eslint --no-color -o ./eslint-report.log ./"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-transform-strict-mode": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babelify": "^10.0.0",
    "browserify": "^16.5.0",
    "bundle-collapser": "^1.3.0",
    "eslint": "^6.6.0",
    "eslint-config-nebenan-base": "^1.0.2",
    "loose-envify": "^1.4.0",
    "terser": "^4.3.9",
    "uglifyify": "^5.0.2"
  },
  "dependencies": {
    "escape-string-regexp": "^2.0.0"
  }
}
